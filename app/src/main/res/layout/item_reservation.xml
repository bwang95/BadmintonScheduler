<?xml version="1.0" encoding="utf-8"?>
<com.cerridan.badmintonscheduler.view.ReservationItemView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingStart="@dimen/global_padding"
    tools:ignore="RtlSymmetry"
    >
  <View
      android:layout_alignParentTop="true"
      style="@style/Widget.BadmintonScheduler.Divider"
      />
  <TextView
      android:id="@+id/tv_reservation_names"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_alignParentStart="true"
      android:layout_centerVertical="true"
      android:layout_margin="@dimen/global_padding"
      android:layout_toStartOf="@+id/tv_reservation_time"
      tools:text="Name1, Name2"
      />
  <TextView
      android:id="@+id/tv_reservation_time"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_alignParentEnd="true"
      android:layout_centerVertical="true"
      android:layout_marginEnd="@dimen/global_padding"
      tools:text="at 7:15 PM"
      />
</com.cerridan.badmintonscheduler.view.ReservationItemView>
